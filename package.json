{
  "name": "gatsby-starter-minimal",
  "version": "1.0.0",
  "private": true,
  "description": "A Gatsby starter with no plugins and a single welcome page.",
  "author": "Laurie Barth",
  "keywords": [
    "gatsby"
  ],
  "scripts": {
    "develop:gatsby": "cross-env GATSBY_GRAPHQL_IDE=playground gatsby develop",
    "develop:type-check": "tsc --watch",
    "develop": "npm-run-all --parallel develop:gatsby develop:type-check",
    "build": "gatsby build",
    "serve": "gatsby serve",
    "clean": "gatsby clean"
  },
  "license": "0BSD",
  "dependencies": {
    "gatsby": "^3.13.1",
    "gatsby-plugin-image": "^1.1.0",
    "gatsby-plugin-layout": "^2.14.0",
    "gatsby-plugin-netlify-cms": "^5.14.0",
    "gatsby-plugin-sharp": "^3.14.0",
    "gatsby-plugin-styled-components": "^4.14.0",
    "gatsby-remark-images": "^5.11.0",
    "gatsby-remark-lazy-load": "^1.0.2",
    "gatsby-remark-prismjs": "^5.11.0",
    "gatsby-remark-relative-images": "^2.0.2",
    "gatsby-source-filesystem": "^3.14.0",
    "gatsby-transformer-remark": "^4.11.0",
    "gatsby-transformer-sharp": "^3.14.0",
    "lazysizes": "^5.3.2",
    "netlify-cms-app": "^2.15.46",
    "prismjs": "^1.25.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "styled-components": "^5.3.1",
    "styled-icons": "^10.42.0"
  },
  "devDependencies": {
    "@types/react": "^17.0.24",
    "@types/react-dom": "^17.0.9",
    "@types/styled-components": "^5.1.14",
    "cross-env": "^7.0.3",
    "env-cmd": "^10.1.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.4.1",
    "typescript": "^4.4.3"
  }
}
